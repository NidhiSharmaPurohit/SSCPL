<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="srmicrosystems.cnote.ManifestGenActivity">
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="srmicrosystems.cnote.ManifestGenActivity"
    android:background="@drawable/back">
    <ListView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:id="@+id/listview"
        tools:context="srmicrosystems.cnote.ManifestItemsActivity"
        android:background="#ab0a0a0a"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="10dp"

        android:padding="0dp"
        android:paddingLeft="0dp"
        android:paddingTop="0dp"
        android:paddingRight="0dp"
        android:paddingBottom="10dp"></ListView>
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab"

        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
android:padding="0dp"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="5dp"
        android:layout_width="56dp"
        android:layout_height="56dp"

        android:src="@drawable/delivery_shipping_truck"
        android:layout_margin="0dp"></android.support.design.widget.FloatingActionButton>


</RelativeLayout>
</android.support.design.widget.CoordinatorLayout>