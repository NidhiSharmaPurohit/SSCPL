          android:layout_toStartOf="@+id/UploadFULLBTN"
          />-->
      <EditText
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:text="1"
          android:id="@+id/updateqty"
          android:textSize="40sp"
        android:layout_toLeftOf="@+id/UploadBTN"
          android:textColor="#c0ff04" />
      <ImageButton
          android:layout_width="65dp"
          android:layout_height="65dp"
          android:src="@drawable/single_package"
          android:scaleType="fitCenter"
          android:id="@+id/UploadBTN"
          android:text="Upload"
          android:padding="10dp"
          android:layout_toStartOf="@+id/UploadFULLBTN"
          android:background="@android:color/transparent" />

<ImageButton
    android:layout_width="65dp"
    android:layout_height="65dp"
    android:src="@drawable/box_icon"
    android:id="@+id/UploadFULLBTN"
    android:text="FUll Load"
    android:layout_alignParentEnd="true"
    android:scaleType="fitCenter"
    android:background="@android:color/transparent" />

    </RelativeLayout>



</GridLayout>
