        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2016150412123"
            android:id="@+id/CNNumber"

            android:textSize="20sp"
            android:textIsSelectable="true"
            android:textColor="#3860ef" />
        <TextView
            android:layout_width="360dp"
            android:layout_height="wrap_content"
            android:text="FedEx Suart"
            android:id="@+id/ConsignerDetails"
            android:layout_below="@id/CNNumber"
            android:textSize="25sp"
            android:textIsSelectable="false"
            android:textColor="#6eec6a" />



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="BOM"
            android:id="@+id/destcc"
            android:textSize="35sp"
            android:layout_alignParentTop="true"
            android:layout_marginStart="10dp"
            android:textColor="#fff700"
            android:layout_toLeftOf="@+id/uqty" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="(Mumbai)"
            android:id="@+id/dsetcity"

            android:layout_marginStart="10dp"
            android:textSize="10sp"
            android:layout_below="@+id/destcc"
            android:layout_alignStart="@+id/destcc"
            android:textColor="#ffffff" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="-"
            android:id="@+id/dash"
            android:textSize="40sp"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_toLeftOf="@id/destcc"
            android:textColor="#ffffff" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="SRT"
            android:id="@+id/fromcc"
            android:textSize="35sp"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@+id/dash"
            android:layout_marginStart="20dp"
            android:textColor="#61f135" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="(Surat)"
            android:id="@+id/fromc"

            android:layout_marginStart="20dp"
            android:textSize="10sp"
            android:layout_below="@+id/fromcc"
            android:layout_alignStart="@+id/fromcc"
            android:textColor="#ffffff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="80"
            android:id="@+id/uqty"
            android:textSize="35sp"
            android:layout_alignParentTop="true"
            android:layout_centerInParent="true"
            android:layout_marginStart="10dp"
            android:textColor="#ff0000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="/"
            android:id="@+id/slash"
            android:textSize="35sp"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/uqty"
            android:layout_marginStart="0dp"
            android:textColor="#ffffff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="100"
            android:id="@+id/tqty"
            android:textSize="35sp"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/slash"
            android:layout_marginStart="0dp"
            android:textColor="#14b5ff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2016150412123"
            android:id="@+id/BookingDate"

            android:textSize="20sp"
            android:layout_toStartOf="@+id/updateqty"
            android:textDirection="rtl"
            android:textColor="#f028c5" />
        <TextView
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="FedEx Mumai"
            android:id="@+id/tocon"
            android:layout_below="@id/BookingDate"
            android:textDirection="rtl"
            android:textSize="25sp"
            android:textIsSelectable="false"
            android:layout_alignEnd="@+id/BookingDate"
            android:layout_toEndOf="@+id/tqty"
            android:textColor="#fb7522" />
     <!-- <Button
          android:layout_width="65dp"
